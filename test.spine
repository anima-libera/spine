-- Hello world!!
kwps "Hello, World!\n";

{
	-- All of these print `a`, they all demonstrate different literal syntaxes.
	kwpc 'a';        -- character
	kwpc '\x61';     -- character with two-hexadecimal-digits ASCII escape
	kwpc '\u{61}';   -- character with hexadecimal-digits Unicode escape
	kwpc '\d{97}';   -- character with decimal-digits Unicode escape
	kwpc 97;         -- decimal
	kwpc 0x61;       -- hexadecimal
	kwpc 0b01100001; -- binary
	kwpc 0r{8}141;   -- arbitrary radix (here 8, so octal)
	kwpc 0r{36}2p;   -- arbitrary radix (here 36)

	kwpc '\n'; @;
}

-- Can do this too!
kwpc kwpc kwpc kwpc 'o' 'w' 'o' '\n';

-- Empty statement, very useful >w<
;

{;;;;;} -- Epic block statement

--{ Block comment
    that can span multiple lines }

-- Terminates process execution. Segfault probably occurs if it is missing.
kwexit;
